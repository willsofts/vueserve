<form name="fslistform" id="fslistform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="programid" />
</form>
<form name="fssortform" id="fssortform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="orderDir" value="<%=page.invertOrder()%>"/>
	<input type="hidden" name="orderBy" />
</form>
<table id="datatable" class="table table-bordered table-hover table-striped tablesorter">
	<thead>
		<tr>
			<th width="75px" class="text-center th-sequence"><label id="seqno_headerlabel"><%=label.get('seqno_headerlabel','No.')%></label></th>
			<th width="150px" class="text-center th-data"><a href="javascript:void(0)" class="alink-sorterr fa-data-sort" data-sorter="curtime"><label id="curtime_headerlabel"><%=label.get('curtime_headerlabel','Date Time')%></label></a></th>
			<th width="100px" class="text-center th-data"><a href="javascript:void(0)" class="alink-sorterr fa-data-sort" data-sorter="userid"><label id="userid_headerlabel"><%=label.get('userid_headerlabel','User')%></label></a></th>
			<th width="120px" class="text-center th-data" nowrap><a href="javascript:void(0)" class="alink-sorterr fa-data-sort" data-sorter="progid"><label id="programid_headerlabel"><%=label.get('programid_headerlabel','Program')%></label></a></th>
			<th width="250px" class="text-center th-data"><label id="progname_headerlabel"><%=label.get('progname_headerlabel','Program Name')%></label></th>
			<th width="100px" class="text-center th-data"><a href="javascript:void(0)" class="alink-sorterr fa-data-sort" data-sorter="action"><label id="action_headerlabel"><%=label.get('action_headerlabel','Action')%></label></a></th>
			<th width="350px" class="text-center th-data"><label id="remark_headerlabel"><%=label.get('remark_headerlabel','Remark')%></label></th>
		</tr>
	</thead>
	<tbody id="datatablebody">							
		<%	let rows = data.dataset?.rows;
			if(rows && rows.length>0) { 
				for(let index=0; index < rows.length; index++) { 
					let item = rows[index]; %>
					<tr>
							<td class="text-center"><%=page.recordsNumber(index+1)%></td>
							<td nowrap><%=page.format({value: item?.curtime, field:{type:"DATETIME"}})%></td>
							<td class="text-center"><%=item?.username%></td>
							<td><%=item?.progid%></td>
							<td><%=item?.progname%></td>
							<td><%=item?.action%></td>
							<td><%=item?.remark%></td>
					</tr>
		<%		}
			} else {
				if(rows) { %>
					<tr>
						<td class="text-center" colspan="7">
							<%=label.get('recordnotfound','Record not found')%>
						</td>
					</tr>
		<%		}
			}	%>
	</tbody>
</table>	
<div id="fschaptertablelayer" class="fschaptertablelayer">
<table id="fschaptertable" class="fschaptertable">
	<tr class="fschapterrow"><td class="fschaptercolumn">
	<form name="fschapterform" id="fschapterform" method="post" autocomplete="off">
		<input type="hidden" name="ajax" value="true"/>
		<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
		<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
		<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
		<input type="hidden" name="orderDir" value="<%=page.setting?.orderDir%>"/>
		<input type="hidden" name="orderBy" value="<%=page.setting?.orderBy%>"/>
	</form>
	<div id="fschapterlayer">
		<%- page.createPaging() %>
	</div>
	</td>
	</tr>
</table>
</div>	
