<form name="fslistformhistory" id="fslistform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="userid" value="<%=data.meta?.userid%>"/>
</form>
<form name="fssortformhistory" id="fssortform" method="post" autocomplete="off">
	<input type="hidden" name="ajax" value="true"/>
	<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
	<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
	<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
	<input type="hidden" name="orderDir" value="<%=page.invertOrder()%>"/>
	<input type="hidden" name="orderBy" />
	<input type="hidden" name="userid" value="<%=data.meta?.userid%>"/>
</form>
<table id="datatablehistory" class="table table-bordered table-hover table-striped tablesorter">
	<thead>
		<tr>
			<th class="text-center th-sequence"><label id="seqno_headerlabel"><%=label.get('seqno_headerlabel','No.')%></label></th>
			<th class="text-left th-data"><a href="javascript:void(0)" class="alink-sorter fa-data-sort" data-sorter="email" onclick="submitOrderHistory(this,'email')"><label id="email_headerlabel"><%=label.get('email_headerlabel','Email')%></label></a></th>
			<th class="text-left th-data"><a href="javascript:void(0)" class="alink-sorter fa-data-sort" data-sorter="issuer" onclick="submitOrderHistory(this,'issuer')"><label id="issuer_headerlabel"><%=label.get('issuer_headerlabel','Issuer')%></label></a></th>
			<th class="text-center th-data"><a href="javascript:void(0)" class="alink-sorter fa-data-sort" data-sorter="createdatetime" onclick="submitOrderHistory(this,'createdatetime')"><label id="createdatetime_headerlabel"><%=label.get('createdatetime_headerlabel','Create Date')%></label></a></th>
			<th class="text-center th-data"><a href="javascript:void(0)" class="alink-sorter fa-data-sort" data-sorter="confirmdatetime" onclick="submitOrderHistory(this,'confirmdatetime')"><label id="confirmdatetime_headerlabel"><%=label.get('confirmdatetime_headerlabel','Confirm Date')%></label></a></th>
		</tr>
	</thead>
	<tbody id="datatablebodyhistory">							
		<%	let rows = data.dataset?.rows;
			if(rows && rows.length>0) { 
				for(let index=0; index < rows.length; index++) { 
					let item = rows[index]; %>
					<tr>
						<td class="text-center"><%=page.recordsNumber(index+1)%></td>
						<td class="text-left"><a href="javascript:void(0)" class="alink-data"><%=item?.email%></a></td>
						<td class="text-left"><a href="javascript:void(0)" class="alink-data"><%=item?.issuer%></a></td>
						<td class="text-center"><a href="javascript:void(0)" class="alink-data"><%=page.format({value: item?.createdate, field:{type:"DATE"}})%> &nbsp; <%=page.format({value: item?.createtime, field:{type:"TIME"}})%></a></td>
						<td class="text-center"><a href="javascript:void(0)" class="alink-data"><%=page.format({value: item?.confirmdate, field:{type:"DATE"}})%> &nbsp; <%=page.format({value: item?.confirmtime, field:{type:"TIME"}})%></a></td>
					</tr>
				<%		}
			} else {
				if(rows) { %>
					<tr>
						<td class="text-center" colspan="5">
							<%=label.get('recordnotfound','Record not found')%>
						</td>
					</tr>
		<%		}
			}	%>			
	</tbody>
</table>	
<div id="fschaptertablelayerhistory" class="fschaptertablelayer">
	<table id="fschaptertablehistory" class="fschaptertable">
		<tr class="fschapterrowhistory">
			<td class="fschaptercolumn">
				<form name="fschapterformhistory" id="fschapterformhistory" method="post" autocomplete="off">
					<input type="hidden" name="ajax" value="true"/>
					<input type="hidden" name="rowsPerPage" value="<%=page.setting?.rowsPerPage%>"/>
					<input type="hidden" name="limit" value="<%=page.setting?.limit%>"/>
					<input type="hidden" name="page" value="<%=page.setting?.page%>"/>
					<input type="hidden" name="orderDir" value="<%=page.setting?.orderDir%>"/>
					<input type="hidden" name="orderBy" value="<%=page.setting?.orderBy%>"/>
					<input type="hidden" name="userid" value="<%=data.meta?.userid%>"/>
				</form>
				<div id="fschapterlayerhistory">
					<%- page.createPaging({jsFunction: "submitChapterHistory", jsForm: "fschapterformhistory"}) %>
				</div>
			</td>
		</tr>
	</table>
</div>
