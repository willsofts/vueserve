<div class="row row-height">
	<div class="col-md-12 col-height">
			<div class="table-layer-class">
				<div class="row row-height">
					<div class="col-height col-md-3">
<label for="keynames" id="keynames_label" class="control-label"><%=label.get('keynames_label','Name')%></label>
<input type="text" id="keynames" name="keyname" class="form-control input-md" maxlength="100" value="<%=data.dataset?.keyname%>" /><span></span>
					</div>
					<div class="col-height col-md-3">
<label for="fromdates" id="fromdates_label" class="control-label"><%=label.get('fromdates_label','From Date')%></label>
<%-page.html('date',{id: "fromdates", name: "fromdate", class: "form-control input-md", current: true})%>
					</div>
					<div class="col-height col-md-3">
<label for="todates" id="todates_label" class="control-label"><%=label.get('todates_label','To Date')%></label>
<%-page.html('date',{id: "todates", name: "todate", class: "form-control input-md", current: true})%>
					</div>
					<div class="col-height col-md">
						<br/>
						<button type="button" id="searchbutton" class="btn btn-dark btn-sm"><i class="fa fa-search fa-btn-icon" aria-hidden="true"></i><%=label.get('searchbutton','Search')%></button>
						<button type="button" id="insertbutton" class="btn btn-dark btn-sm pull-right"><i class="fa fa-plus fa-btn-icon" aria-hidden="true"></i><%=label.get('insertbutton','Insert')%></button>
					</div>
				</div>
			</div>
	</div>
</div>
