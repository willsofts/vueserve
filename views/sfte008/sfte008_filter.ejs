<div class="filter-layer">
	<div class="row row-height">
		<div class="col-md-3 col-height col-label">
			<label id="groupname_label" class="control-label"><%=label.get('groupname_label','Group')%></label>
		</div>
		<div class="col-md-3 col-height">
			<%-page.html('select',{map: data.entity?.tgroup, showing: "both", value: data.dataset?.groupname, id: "groupname", name: "groupname", class: "form-control input-md"})%>
		</div>
		<div class="col-md-2 ">
			<button type="button" id="explorebutton" class="btn btn-dark btn-sm"><i class="fa fa-search fa-btn-icon" aria-hidden="true"></i><%=label.get('explorebutton','Explore')%></button>
		</div>
	</div>
</div>
